<?xml version="1.0" encoding="utf-8" ?>
<!--
DO NOT EDIT THIS FILE. IT WILL GET OVERWRITTEN IN AN UPDATE.

Instead, create your own. See the docs:
https://docs.phpvms.net/acars/configmaps

Contributed by B.Fatih KOZ <https://github.com/FatihKoz>

By default all lights are ignored for FsLabs Airbus Series due to
their usage of SimConnect Lvars instead of FsUipc offsets.

Also CONF CHG indexes added to cover flap/slat movements they report
as flap positions.
-->
<Rules>
  <Rule Simulator="Prepar3d" TitleContains="FSLabs">
    <NavigationLights Ignore="True" />
    <BeaconLights Ignore="True" />
    <TaxiLight Ignore="True" />
    <StrobeLights Ignore="True" />
    <LandingLights Ignore="True" />
    <Flaps>
      <Flap Index="0" Text="UP" />
      <Flap Index="1" Text="CONF 1" />
      <Flap Index="2" Ignore="True" Text="CONF CHG" />
      <Flap Index="3" Ignore="True" Text="CONF CHG" />
      <Flap Index="4" Ignore="True" Text="CONF CHG" />
      <Flap Index="5" Ignore="True" Text="CONF CHG" />
      <Flap Index="6" Text="CONF 1+F" />
      <Flap Index="7" Text="CONF 2" />
      <Flap Index="8" Text="CONF 3" />
      <Flap Index="9" Text="FULL" />
    </Flaps>
  </Rule>
  <Rule Simulator="FSX" TitleContains="FSLabs">
    <NavigationLights Ignore="True" />
    <BeaconLights Ignore="True" />
    <TaxiLight Ignore="True" />
    <StrobeLights Ignore="True" />
    <LandingLights Ignore="True" />
    <Flaps>
      <Flap Index="0" Text="UP" />
      <Flap Index="1" Text="CONF 1" />
      <Flap Index="2" Ignore="True" Text="CONF CHG" />
      <Flap Index="3" Ignore="True" Text="CONF CHG" />
      <Flap Index="4" Ignore="True" Text="CONF CHG" />
      <Flap Index="5" Ignore="True" Text="CONF CHG" />
      <Flap Index="6" Text="CONF 1+F" />
      <Flap Index="7" Text="CONF 2" />
      <Flap Index="8" Text="CONF 3" />
      <Flap Index="9" Text="FULL" />
    </Flaps>
  </Rule>
</Rules>
