<Rules>
  <Rule Simulator="X-Plane" TitleContains="Rotate MD-80">
    <BeaconLights>
      <Key Type="Int" Key="Rotate/md80/lights/anticollision_light_switch" Value="1"/>
    </BeaconLights>
    <LandingLights>
      <Key Type="Int" Key="Rotate/md80/lights/wing_ldg_light_switch_r" Value="2"/>
      <Key Type="Int" Key="Rotate/md80/lights/wing_ldg_light_switch_l" Value="2"/>
    </LandingLights>
    <NavigationLights>
      <Key Type="Int" Key="Rotate/md80/lights/pos_strobe_light_switch" Value="1|2"/>
    </NavigationLights>
    <StrobeLights>
      <Key Type="Int" Key="Rotate/md80/lights/pos_strobe_light_switch" Value="2"/>
    </StrobeLights>
    <TaxiLight>
      <Key Type="Int" Key="Rotate/md80/lights/nose_light_switch" Value="1|2"/>
    </TaxiLight>
    <WingLight>
      <Key Type="Int" Key="Rotate/md80/lights/wing_light_switch" Value="1"/>
    </WingLight>
    <LogoLight>
      <Key Type="Int" Key="Rotate/md80/lights/logo_light_switch" Value="1"/>
    </LogoLight>
  </Rule>
</Rules>