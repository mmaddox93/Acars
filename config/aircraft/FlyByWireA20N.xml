<Rules>
  <Rule Simulator="Microsoft Flight" TitleContains="A320 Neo FlyByWire">
    <NavigationLights>
      <Key Type="Boolean" Key="LIGHT NAV ON" />
    </NavigationLights>
    <BeaconLights>
      <Key Type="Boolean" Key="LIGHT BEACON ON" />
    </BeaconLights>
    <TaxiLight>
      <Key Type="Boolean" Key="LIGHT TAXI ON" />
    </TaxiLight>
    <StrobeLights>
      <Key Type="Boolean" Key="LIGHT STROBE ON" />
    </StrobeLights>
    <LandingLights>
      <Key Type="Boolean" Key="LIGHT LANDING ON" />
    </LandingLights>
    <Flaps>
      <Flap Index="0" Text="UP" />
      <Flap Index="1" Text="CONF 1" />
      <Flap Index="2" Text="CONF 1+F" />
      <Flap Index="3" Text="CONF 2" />
      <Flap Index="4" Text="CONF 3" />
      <Flap Index="5" Text="FULL" />
    </Flaps>
  </Rule>
</Rules>
